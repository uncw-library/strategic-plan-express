<div class="col col-xxl-3 col-xl-4 col-lg-4 col-md-12 col-12">
    <form>
        <div class="mb-3">
            <label for="status" class="form-label">Status</label>
            <select class="form-select" multiple aria-label="status select" id="status">
                {{#each searchOptionsData.status as |item|}}
                  <option value="{{item}}">{{item}}</option>
                {{/each}}
            </select>
            <div id="emailHelp" class="form-text">You may select more than one in each box.</div>
        </div>
        <div class="mb-3">
            <label for="target-year" class="form-label">Target Year</label>
            <select class="form-select" multiple size="3" aria-label="status select" id="year">
                {{#each searchOptionsData.target_academic_year as |item|}}
                  <option value="{{item}}">{{item}}</option>
                {{/each}}
            </select>
        </div>
        <div class="mb-3">
            <label for="members" class="form-label">Members</label>
            <select class="form-select" multiple size="3" aria-label="status select" id="members">
                {{#each searchOptionsData.members_and_leads as |item|}}
                  <option value="{{item}}">{{item}}</option>
                {{/each}}
            </select>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>